@model HomeViewModel


<partial name="_Carousel" />

<h2>Desert of the week</h2>
<h4>Our weekly selection - just for you!</h4>

<div class="row">

    @foreach (var pie in Model.PiesOfTheWeek)
    {
         <partial name="_PieCard" model="pie" />
    }
</div>
<div class="row">

    @foreach (var cake in Model.CakesOfTheWeek)
    {
<partial name="_CakeCard" model="cake" />}
</div>